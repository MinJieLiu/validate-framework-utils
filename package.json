{
  "name": "validate-framework-utils",
  "version": "0.2.1",
  "description": "Some utils are used for validation.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src --fix",
    "deploy": "npm run clean && better-npm-run prod",
    "test": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "betterScripts": {
    "prod": {
      "command": "babel src --out-dir lib --source-maps --extensions .js --copy-files",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MinJieLiu/validate-framework-utils.git"
  },
  "keywords": [
    "validate-framework-utils"
  ],
  "author": "LMY",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MinJieLiu/validate-framework-utils/issues"
  },
  "homepage": "https://github.com/MinJieLiu/validate-framework-utils#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.13",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4"
  }
}
